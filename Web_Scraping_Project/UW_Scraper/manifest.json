{
    "name": "UW Degree Audit Tool",
    "version": "0.01",
    "description": "--",
    "content_scripts": [
        {
            "matches": ["https://myplan.uw.edu/audit/*"],
            "js": ["contentScript.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "manifest_version": 3
}